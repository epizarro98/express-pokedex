<h1>Favorite Pokemon</h1>

<ul>
    <% favorites.forEach((favorite)=>{ %>
        <li>
            <%= favorite.name %> 
            <form method="POST" action="/pokemon/<%=favorite.id%>/?_method=DELETE">
                <input type="submit" value="Delete">
            </form>
        </li>
    <% })  %> 
</ul>